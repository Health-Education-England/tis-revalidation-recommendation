{
  "containerDefinitions": [
    {
      "name": "reval-be",
      "image": "430723991443.dkr.ecr.eu-west-2.amazonaws.com/tis-hee-revalidation/tis-hee-revalidation:1",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "TITLE",
          "value": "reval-be"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-2"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-reval-be",
          "awslogs-region": "eu-west-2",
          "awslogs-stream-prefix": "reval-be"
        }
      },
      "secrets": [
        {
          "name": "MONGODB_HOST",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/revalidation-db-host"
        },
        {
          "name": "MONGODB_PORT",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/revalidation-db-port"
        },
        {
          "name": "MONGODB_USERNAME",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/revalidation-db-user"
        },
        {
          "name": "MONGODB_PASSWORD",
          "valueFrom": "arn:aws:ssm:eu-west-2:430723991443:parameter/revalidation-db-password"
        }
      ]
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "reval-be",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
